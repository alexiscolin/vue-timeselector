{
  "name": "vue-timeselector",
  "version": "0.0.3",
  "description": "Simple customizable Vue.js timepicker component",
  "author": "Alexis Colin <nesquimo@gmail.com>",
  "repository": "git+https://github.com/alexiscolin/vue-timeselector.git",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vue",
    "timepicker",
    "timeselector",
    "date",
    "time",
    "custom",
    "vuejs2"
  ],
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1",
    "vue-jest": "^3.0.2",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^2.1.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
